   "source": [
    "def fibonacci_recursive(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    else :\n",
    "        return fibonacci_recursive(n - 1) + fibonacci_recursive(n - 2)\n",
    "\n",
    "\n",
    "\n",
    "# Example \n",
    "n = 10 \n",
    "for i in range(2, n):\n",
    "    print(fibonacci_recursive(i)) \n"
   ]
  }
