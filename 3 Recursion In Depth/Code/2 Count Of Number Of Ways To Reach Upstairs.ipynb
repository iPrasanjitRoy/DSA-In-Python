   "source": [
    "## Helper method\n",
    "def helper(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    return helper(n-1) + helper(n-2)\n",
    "\n",
    "## function definition\n",
    "def countNumberOfWays(n):\n",
    "    return helper(n+1)\n",
    "\n",
    "## Driver code\n",
    "n = 4\n",
    "result = countNumberOfWays(n)\n",
    "print(\"Number of ways to reach upstairs is\",result)"
   ]
  }
